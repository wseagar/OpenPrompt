{
  "name": "openprompt",
  "version": "0.1.0",
  "description": "A JavaScript implementation of the OpenPrompt specification",
  "contributors": [
    "Will Seagar <w.seagar@gmail.com>"
  ],
  "repository": "git://github.com/wseagar/OpenPrompt.git",
  "license": "MIT",
  "keywords": [
    "typescript",
    "esbuild"
  ],
  "main": "./dist/esbuild/node.js",
  "types": "./dist/tsc/main.d.ts",
  "browser": "./dist/esbuild/browser.js",
  "scripts": {
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "test-watch": "jest --watchAll",
    "clean": "rm -rf dist build package",
    "docs": "typedoc --entryPoints src/main.ts",
    "build-all": "npm run clean && tsc --emitDeclarationOnly -p tsconfig.json && npx ts-node build.ts",
    "build-and-publish": "npm run build-all && npm publish"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "dotenv": "^10.0.0",
    "esbuild": "^0.11.11",
    "eslint": "^7.22.0",
    "jest": "^26.6.3",
    "postinstall-postinstall": "^2.1.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "liquidjs": "^10.7.0"
  }
}
